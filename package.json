{
  "name": "sdedit",
  "version": "0.1.2",
  "description": "A wrapper to use sdedit via nodejs",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "postinstall": "node sdeditIfInstalled.js update",
    "clean": "rimraf \"+(test|src)/**/*+(.d.ts|.js|.map)\" .nyc_output reports coverage",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "postbuild": "tslint -p tsconfig.json && node bin/sdedit update",
    "pretest": "npm run build",
    "test": "nyc --check-coverage --reporter=html --report-dir=reports/coverage --lines 70 --functions 80 --branches 75 mocha \"test/helpers/**/*.js\" \"test/unit/**/*.js\" \"test/integration/**/*.js\"",
    "posttest": "npm run stryker",
    "stryker": "stryker run",
    "preversion": "npm t",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "npm publish && git push && git push --tags",
    "release:patch": "npm version patch -m \"chore(release): %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicojs/node-sdedit.git"
  },
  "keywords": [
    "sdedit",
    "sequence",
    "diagram",
    "sddiagram"
  ],
  "bin": {
    "sdedit": "./bin/sdedit"
  },
  "license": "Apache-2.0",
  "author": "Nico Jansen <jansennico@gmail.com>",
  "bugs": {
    "url": "https://github.com/nicojs/node-sdedit/issues"
  },
  "homepage": "https://github.com/nicojs/node-sdedit#readme",
  "devDependencies": {
    "@stryker-mutator/core": "^1.1.1",
    "@stryker-mutator/html-reporter": "^1.1.1",
    "@stryker-mutator/mocha-framework": "^1.1.1",
    "@stryker-mutator/mocha-runner": "^1.1.1",
    "@stryker-mutator/typescript": "^1.1.1",
    "@types/chai": "^4.1.7",
    "@types/end-of-stream": "^1.4.0",
    "@types/got": "^9.4.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "@types/sinon": "^5.0.5",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.12",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.1",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.11",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "download": "^7.1.0",
    "end-of-stream": "^1.4.1",
    "got": "^9.6.0"
  }
}
